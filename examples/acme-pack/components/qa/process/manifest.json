{
  "id": "qa.process",
  "version": "0.1.0",
  "supports": ["messaging"],
  "world": "acme:qa/process",
  "profiles": {
    "default": "standard",
    "supported": ["standard"]
  },
  "capabilities": {
    "wasi": {
      "random": true,
      "clocks": true
    },
    "host": {
      "secrets": {
        "required": ["SLACK_BOT_TOKEN", "TEAMS_CLIENT_SECRET"]
      },
      "telemetry": {
        "scope": "tenant"
      }
    }
  }
}
